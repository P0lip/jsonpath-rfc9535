{
  "document": ["a", "b", "c", "d", "e", "f", "g"],
  "queries": [
    {
      "path": "$[1:3]",
      "values": ["b", "c"]
    },
    {
      "path": "$[5:]",
      "values": ["f", "g"]
    },
    {
      "path": "$[1:5:2]",
      "values": ["b", "d"]
    },
    {
      "path": "$[5:1:-2]",
      "values": ["f", "d"]
    },
    {
      "path": "$[::-1]",
      "values": ["g", "f", "e", "d", "c", "b", "a"]
    }
  ]
}
